---
layout: default
---

<div
  class="text-center"
  x-data="{ tab: window.location.hash ? window.location.hash.substring(1) : 'line' }"
  id="tab_wrapper"
>
  <!-- The tabs content -->
  <div class="container mx-auto px-4">
      <div
        class="icon-wrapper"
        style="grid-template-columns: repeat(auto-fill, minmax(110px, 1fr))"
      >
      {% for icon in iconsNew %}
      <div class="icon-container">
        <div class="my-4">
          {{ icon.sourceWithClass | safe }}
        </div>
        <div class="icon-code" id="{{ icon.nameWithoutExtension }}">
          {{ icon.source }}
        </div>
        <!-- <div class="icon-name">{{ icon.nameWithoutExtension }}</div> -->
        <div
          class="icon-copy-trigger group-hover:opacity-100"
          data-clipboard-target="#{{ icon.nameWithoutExtension }}"
        >
          Copy SVG
        </div>
      </div>
      {% endfor %}
  </div>
</div>
