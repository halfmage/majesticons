{
  "name": "majesticons",
  "version": "1.0.0",
  "description": "A versitile, beautiful and scalable iconset. 210 icons each in line and solid.",
  "main": "index.js",
  "scripts": {
    "start": "eleventy --serve & postcss styles/tailwind.css --o _tmp/style.css --watch",
    "build": "ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production postcss styles/tailwind.css --o _site/style.css && cleancss _site/style.css -o _site/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halfmage/majesticons.git"
  },
  "keywords": [
    "icons",
    "vector",
    "svg"
  ],
  "author": "@halfmage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halfmage/majesticons/issues"
  },
  "homepage": "https://github.com/halfmage/majesticons#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "alpinejs": "^2.8.0",
    "autoprefixer": "^10.2.4",
    "clean-css-cli": "^4.3.0",
    "clipboard": "^2.0.6",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "fast-glob": "^3.2.5",
    "html-minifier": "^4.0.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  }
}
